{
  "hosting": {
    "public": "dist/dartplayer-web/",
    "404": "",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "run": {
          "serviceId": "dartplayer",
          "region": "southamerica-east1"
        }
      }
    ]
  }
}
